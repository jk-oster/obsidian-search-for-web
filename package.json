{
  "name": "obsidian-browser-search",
  "description": "Search your Obsidian vault simultaneously as you type your search in your favorite search engine.",
  "version": "2.5.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-firefox": "cross-env TARGET=firefox vite",
    "check-types": "vue-tsc",
    "build": "npm run check-types && npm run build-chrome && npm run build-firefox && npm run zip",
    "build-chrome": "vite build",
    "build-firefox": "cross-env TARGET=firefox vite build",
    "rename": "mv $(find ./dist/ -name '_plugin-vue_export-helper.js') ./dist/plugin-vue_export-helper.js",
    "rewrite": "cd ./dist && grep -Ril '_plugin-vue_export-helper\\.js' | xargs sed -i 's/_plugin-vue_export-helper\\.js/plugin-vue_export-helper\\.js/g' && cd ../",
    "zip": "zip -r dist.zip dist/",
    "post-version": "git push && git push --tags",
    "version-patch": "npm version patch --force -m \"[RELEASE] patch version %s\" && npm run build",
    "version-minor": "npm version minor --force -m \"[RELEASE] minor version %s\" && npm run build",
    "version-major": "npm version major --force -m \"[RELEASE] major version %s\" && npm run build",
    "deploy": "npm run ./deploy.js",
    "upload": "npm run ./upload.js",
    "postinstall": "patch-package",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.3",
    "@splidejs/vue-splide": "^0.6.12",
    "@vueuse/core": "^11.1.0",
    "@webext-core/isolated-element": "^1.1.2",
    "@webext-core/messaging": "^2.1.0",
    "@webext-core/proxy-service": "^1.2.1",
    "@webext-core/storage": "^1.2.0",
    "codemirror": "^6.0.1",
    "easymde": "^2.18.0",
    "flowbite": "^3.1.1",
    "marked": "^15.0.6",
    "vue": "^3.3.4",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/chrome": "^0.0.301",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.18",
    "chrome-webstore-upload": "^3.1.4",
    "cross-env": "^7.0.3",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.35",
    "postinstall-postinstall": "^2.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-web-extension": "^4.0.0",
    "vitepress": "^1.6.3",
    "vue-tsc": "^2.1.6",
    "zip-folder": "^1.0.0"
  }
}
